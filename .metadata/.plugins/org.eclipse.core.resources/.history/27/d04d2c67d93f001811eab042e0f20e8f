package com.course.two.jframe;

import javax.swing.JFrame;
public class GUIDemo {
	static void print() {
		JFrame jf = new JFrame();
		
		jf.setVisible(true);
		jf.setSize(200, 300);
		jf.setLocation(500, 300);
		jf.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
	public static void main(String[] args) {
		print();
	}
}
