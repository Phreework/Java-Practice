package com.course.two;

class MuiltTableDemo {
	public static void main(String[] args) {
		// 循环得到9*9乘法表
		for (int x = 1; x <= 9; x++) {
			for (int y = 1; y <= x; y++) {
				System.out.print(y + " * " + x + " = " + (x * y) + "\t");
			}
			System.out.println();
		}
	}
	
	static int[] getPrimeNumber(int min,int max) {
		int[] arr; 
		for(int i = min; i <= max ; i++) {
			 for (int j = 2; j < i; ++j) {  
			        if (i % j == 0) break;  
			 } 
		}
		return arr;
	}
	
}