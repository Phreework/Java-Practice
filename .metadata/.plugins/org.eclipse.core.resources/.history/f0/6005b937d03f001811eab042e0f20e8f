package com.supermarket;

import java.util.Scanner;

import com.supermarket.view.Test.Emenu;

public class AwardMenu extends Menu{
	
	public Emenu print() {
		Scanner s = new Scanner(System.in);
		
		
		System.out.println("****好又多超市欢迎你****");
		System.out.println("\t恭喜中奖");
		System.out.println("\t1.退出");
		System.out.println("********************");
		System.out.println("请输入选择的数字：");
		int i = s.nextInt();
		if (i == 1) 
			return Emenu.主菜单;
		return Emenu.错误;
	}
	AwardMenu(){};
}
