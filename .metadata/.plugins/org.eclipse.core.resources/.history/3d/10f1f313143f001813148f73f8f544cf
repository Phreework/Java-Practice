package com.course.one;

public class Test1 {
	
	public static void main(String[] args) {
		int price = 50;
		int count = 120;
		System.out.println("总价格是：" + calcuPrice(price,count));
	}
	/*
	 * param 价格
	 * param 统计
	 */
	public static int calcuPrice(int price, int count) {
		return price*count;
	}
}
